
@model Employee

<div class="card p-5 shadow w-50 mx-auto mt-5">
    <h5 class="text-center">Are you sure you want to delete this data?</h5>
    <div class="d-flex justify-content-center">
      <form action="/employee/delete" method="post">
          <input type="hidden" asp-for="Id"/>
            <button class="btn btn-danger me-3">YES</button>
      </form>
    <a href="/employee/index"><button class="btn btn-primary">CANCEL</button></a>
    </div>
   

</div>